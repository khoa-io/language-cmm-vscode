{
    "name": "language-cmm-vscode",
    "repository": "https://github.com/khoa-io/language-cmm-vscode",
    "displayName": "PRACTICE Script Language Support",
    "description": "Provides basic syntax highlighting for PRACTICE Script Language",
    "version": "1.0.0",
    "publisher": "khoa-io",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "cmm",
            "aliases": ["PRACTICE", "cmm"],
            "extensions": [".cmm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cmm",
            "scopeName": "source.cmm",
            "path": "./syntaxes/cmm.tmLanguage.json"
        }]
    }
}
