{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "PRACTICE Script Language",
    "patterns": [{
            "include": "#keywords"
        },
        {
            "include": "#strings"
        }
    ],
    "repository": {
        "keywords": {
            "patterns": [{
                "name": "keyword.control.cmm",
                "match": "\\b(?i)(goto|gosub|return|jumpto|do|enddo|run|end|entry|local|global|if|else|while|repeat|wait|on|inkey|print|area|enter|title|beep|read|write|open|close|stop|continue|pbreak|pstep|plist|pedit|pmacro|pbreak)\\b"
            }]
        },
        "strings": {
            "name": "string.quoted.double.cmm",
            "begin": "\"",
            "end": "\"",
            "patterns": [{
                "name": "constant.character.escape.cmm",
                "match": "\\\\."
            }]
        }
    },
    "scopeName": "source.cmm"
}
